{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "solsearch",
  "title": "Solsearch",
  "description": "Search data in one of Solana explorers",
  "icon": "solsearch-logo.png",
  "author": "aquare11e",
  "categories": [
    "Web",
    "Finance"
  ],
  "license": "MIT",
  "keywords": [
    "solsearch",
    "solana"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Solsearch",
      "subtitle": "Search by transaction, address, block",
      "description": "Search in Solana by transaction, address, block",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "explorer",
      "description": "Your preferred Solana explorer",
      "type": "dropdown",
      "required": false,
      "title": "Preferred explorer",
      "default": "SOLSCAN",
      "data": [
        {
          "title": "Solscan",
          "value": "SOLSCAN"
        },
        {
          "title": "Solana Explorer",
          "value": "SOLANA_EXPLORER"
        },
        {
          "title": "Solana FM",
          "value": "SOLANA_FM"
        },
        {
          "title": "Solana Beach",
          "value": "SOLANA_BEACH"
        }
      ]
    },
    {
      "name": "maxStorage",
      "description": "Maximum last values to store",
      "type": "dropdown",
      "required": false,
      "title": "Maximum last values to store",
      "default": "20",
      "data": [
        {
          "title": "10",
          "value": "10"
        },
        {
          "title": "20",
          "value": "20"
        },{
          "title": "50",
          "value": "50"
        },
        {
          "title": "100",
          "value": "100"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}